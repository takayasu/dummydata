<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
	<appenders>
		<Console name="console">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} [%-5p] %m (%C{1}:%L)%n" />
		</Console>
 	</appenders>
	<loggers>
		<root level="info">
			<appender-ref ref="console" />
		</root>
	</loggers>
</configuration>